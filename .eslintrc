{
   "parser": "@typescript-eslint/parser",
   "plugins": ["@typescript-eslint"],
   "extends": ["airbnb"],
   "settings": {
        "import/extensions": [".js",".jsx",".ts",".tsx"],
        "import/parsers": {
          "@typescript-eslint/parser": [".ts",".tsx"]
         },
         "import/resolver": {
             "node": {
                 "extensions": [".jsx", ".tsx"]
             }
         }
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", "tsx"] }]
    },
    "env": {
        "browser": true
    }
}